package <%= @namespace %>.model;

import java.util.ArrayList;
import java.util.Date;

public class <%= @type.class_name %> {

  <% @type.fields.each do |f| %>private <%= f.type %> m_<%= f.name %> = <%= f.default %>;
  <% end %>

  <% @type.fields.each do |f| %>
  public <%= f.type %> get<%= f.name %>() {
    return m_<%= f.name %>;
  }

  public void set<%= f.name %>( <%= f.type %> value ) {
    m_<%= f.name %> = value;
  }
  <% end %>

  @Override
  public boolean equals( Object o ) {
    if( o == null ) {
      return false;
    }

    if( o instanceof <%= @type.class_name %> == false ) {
      return false;
    }
    
    <%= @type.class_name %> other = (<%= @type.class_name %>)o;
    
    <% @type.fields.reject{ |a| ['created', 'modified'].include? a.name.downcase }.each do |f| %>
    if( other.get<%= f.name %>().equals( this.get<%= f.name %>() ) == false ) {
      return false;
    }
    <% end %>

    return true;
  }

}
