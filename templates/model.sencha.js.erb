Ext.regModel('<%= @type.name %>', {
    fields: [
	<% @type.primitive_fields.each do |f| %>{name: '<%= f.name.underscore %>', type: '<%= f.type.downcase %>'},
	<% end %>
	],
		
	associations: [	        
	<% @type.complex_fields.each do |f| %>{type: 'hasMany', model: '<%= f.type %>', name: '<%= f.name.underscore.pluralize %>'},
	<% end %>
	]
});
